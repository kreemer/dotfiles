{{- $codespaces:= env "CODESPACES" | not | not -}}
{{- $devpod:= env "DEVPOD" | not | not -}}
{{- $devcon:= env "DEVCON" | not | not -}}
{{- $container:= (or (env "CODESPACES" | not | not) (env "DEVPOD" | not | not) (env "DEVCON" | not | not) (env "USER" | eq "vscode")) -}}
data:
    tiling: false
    codespaces: {{ $codespaces }}
    devpod: {{ $devpod }}
    devcon: {{ $devcon }}
    container: {{ $container }}
secret:
    command: pass-cli
